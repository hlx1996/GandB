<launch>
  <node name="random" pkg="grad_spline" type="random" output="screen">
    <param name="alpha" value="0.35" type="double"/>
    <param name="beta" value="0.97" type="double"/>
    <param name="lamda" value="0.9" type="double"/>
    <param name="dist0" value="1.2" type="double"/>
    <param name="min_step" value="0.5" type="double"/>
    <param name="max_step" value="1.0" type="double"/>
    <param name="obs_th" value="1.0" type="double"/>

    <param name="opti_num" value="80" type="int"/>
    <param name="sleep" value="0.005" type="double"/>
  </node>
</launch>
 
