<launch>
  <node name="random" pkg="grad_spline" type="random" output="screen">
    <param name="alpha" value="0.6" type="double"/>
    <param name="beta" value="0.95" type="double"/>
    <param name="lamda" value="0.8" type="double"/>
    <param name="dist0" value="0.8" type="double"/>
    <param name="min_step" value="0.2" type="double"/>
    <param name="max_step" value="0.4" type="double"/>
    <param name="obs_th" value="1.5" type="double"/>

    <param name="opti_num" value="1" type="int"/>
    <param name="point_opti_num" value="30" type="int"/>
    <param name="algorithm" value="11" type="int"/>
    <param name="scale" value="1.0" type="double"/>
    <param name="sleep" value="0.001" type="double"/>
    <param name="solver" value="3" type="int"/>
  </node>
</launch>
 
